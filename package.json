        "face-api.js": "^0.22.2",
        "html2canvas": "^1.4.1",
        "jquery": "^3.7.1",
        "jspdf": "^2.5.2",
        "lucide-vue-next": "^0.525.0",
        "pinia": "^3.0.3",
        "reka-ui": "^2.3.2",
