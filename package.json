{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "test:run": "vitest run",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint resources/js --ext .js,.ts,.vue",
        "lint:fix": "eslint resources/js --ext .js,.ts,.vue --fix",
        "format": "prettier --write \"resources/js/**/*.{js,ts,vue,json}\" \"**/*.{php,md}\"",
        "format:check": "prettier --check \"resources/js/**/*.{js,ts,vue,json}\" \"**/*.{php,md}\"",
        "quality": "npm run type-check && npm run lint && npm run format:check && npm run test:run"
    },
    "devDependencies": {
        "@heroicons/vue": "^2.2.0",
        "@prettier/plugin-php": "^0.23.0",
        "@tailwindcss/forms": "^0.5.2",
        "@tailwindcss/typography": "^0.5.16",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/vue": "^8.1.0",
        "@types/node": "^24.0.13",
        "@typescript-eslint/eslint-plugin": "^8.36.0",
        "@typescript-eslint/parser": "^8.36.0",
        "@vue/test-utils": "^2.4.6",
        "alpinejs": "^3.4.2",
        "autoprefixer": "^10.4.2",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "eslint": "^9.31.0",
        "eslint-plugin-vue": "^10.3.0",
        "happy-dom": "^18.0.1",
        "jsdom": "^26.1.0",
        "laravel-vite-plugin": "^1.2.0",
        "postcss": "^8.4.31",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^3.1.0",
        "terser": "^5.43.1",
        "typescript": "^5.8.3",
        "vite": "^6.2.4",
        "vitest": "^3.2.4",
        "vue-eslint-parser": "^10.2.0",
        "vue-tsc": "^3.0.1"
    },
    "dependencies": {
        "@headlessui/vue": "^1.7.23",
